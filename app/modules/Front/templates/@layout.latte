						<li class="active">Registrační systém</li>
					</ol>
				{/block}
			</div>
			<div class="pull-right">
				{if $user->isLoggedIn()}
					Přihlášen:
					<strong>{$me->getFullname()}</strong>
					|
					{if $user->isInRole('serviceteam') && $user->isInRole('database')}
						<a n:href=":Database:Dashboard:">Vstup do databáze</a>
						|
					{/if}
					<a n:href="logout!">Odhlásit se</a>
				{/if}
			</div>
		</div>
	</div>

	<div class="container">
		<div class="row">
			<div class="col-md-12">
				{snippet flash}
					<div n:foreach="$flashes as $flash" class="alert alert-{$flash->type}">
						<button type="button" class="close" data-dismiss="alert">×</button>
						{$flash->message}
					</div>
				{/snippet}

				{include #content}
			</div>
		</div>
	</div>
</section>
